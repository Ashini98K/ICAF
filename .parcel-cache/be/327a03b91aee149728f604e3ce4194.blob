.logoBlock {
    background: url(5739388f83efeb26536d2e72ff8add52) no-repeat center;
    background-size: fill;
    background-color: rgba(255, 255, 255, 0.47);
    width: 150px;
    height: 250px;
    border-radius: 25px;
    background-size: 550px 170px;
}

.bngImage {
    background: url(02dc2d1e7b32f7eeb4cec005c993a899) no-repeat center;
    background-size: cover;
    padding-bottom: 5.6vh;
    padding-right: 4vh;
    width: 100%;
    height: 100%;
    min-height: 100%;
    min-width: 100%;
    align-items: flex-end;
}

.rowstyle {
    padding-top: 7vh;
}

.headTopic {
    font-style: normal;
    font-weight: 550;
    font-size: 55px;
    line-height: 75px;
    color: #FFFFFF;
    text-align: center;
}

.headDate {
    font-family: Post No Bills Colombo;
    font-style: normal;
    font-weight: normal;
    font-size: 55px;
    line-height: 75px;
    color: #FFFFFF;
    text-align: center;
}

.viewEvent {
    background: rgba(44, 110, 148, 0.64);
    border-radius: 63px;
    width: 200px;
    height: 55px;
    font-family: Red Rose;
    font-style: normal;
    font-weight: normal;
    font-size: 30px;
    line-height: 40px;
    color: #FFFFFF;
}